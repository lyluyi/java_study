package cn.sxt.mycollection;

import java.util.HashMap;

// 手动实现一个HashSet 理解HashSet 底层原理

public class SxtHashSet {
		HashMap map;
		
		private static final Object PRESENT = new Object();
		
		public SxtHashSet() {
				map = new HashMap();
		}
		
		public void add(Object o) {
				map = new HashMap(o, PRESENT);
		}
		
}
