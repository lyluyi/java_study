package com.sxt.thread;

/**
 *   创建线程的方式
 * 
 * 1、 创建：继承Thread + 重写run方法
 * 
 * 
 * @author Administrator
 *
 */

public class StartThread extends Thread {
		
		/**
		 * 
		 * 线程入口点
		 * 
		 * */
		
		@Override
		public void run () {
				for (int i = 0; i < 20; i++) {
						System.out.println("一边听歌一边敲代码！");
				}
		}
}
