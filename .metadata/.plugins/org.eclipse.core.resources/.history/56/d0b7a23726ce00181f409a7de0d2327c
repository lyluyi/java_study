package cn.sxt.arrays;

// 数组的遍历 forEach

public class Test03 {
	public static void main() {
			
			int[] arr01 = new int[10];		
			
			for (int i=0; i < arr01.length; i++) {
					arr01[i] = 10*i;
					System.out.println(arr01[i]);
			}
			
		// foreach循环用于读取数组元素的值 不能修改元素的值
			for(int m : arr01) {
				System.out.println(m);
			}
			
	}
}
