/*
 * 飞行接口
 */

interface volant {
	int FLY_HEIGHT = 1000;
	public abstract	void fly();
}

// 善良接口

interface Honest {
	public abstract void helpOther();
}

class Angel implements volant,Honest {
	
	@Override
	
	public void helpOther() {
		
	}
}
