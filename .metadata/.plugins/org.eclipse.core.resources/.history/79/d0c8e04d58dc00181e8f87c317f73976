package com.sxt.io;

import java.io.File;

/*
 * 名称或路径
 * 
 * getName()
 * getPath()
 * getParent()
 * 
 * */

public class FileDemo03 {
		
		public static void main(String[] args) {
				
				File src = new File("IO.jpg");
				
				System.out.println("名称：" + src.getName());  // IO.jpg
				System.out.println("路径：" + src.getPath());  // D:\workspace\IO\src\IO.jpg  返回相对路径
				
		}
		
}
